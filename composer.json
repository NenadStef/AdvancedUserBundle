{
    "name": "nenadstef/advanced-user-bundle",
    "type": "symfony-bundle",
    "description": "NenadStef's Symfony AdvancedUserBundle",
    "keywords": [ "User management" ],
    "homepage": "https://github.com/NenadStef/AdvancedUserBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Stefanovic",
            "email": "stefanovic21@sbb.rs"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "^5.0"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.3",
        "symfony/doctrine-bridge": "<2.7"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/mongodb-odm-bundle": "^4.1",
        "doctrine/orm": "^2.7",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "NenadStef\\AdvancedUserBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NenadStef\\AdvancedUserBundle\\Tests\\": "tests/"
        }
    }
}
